{
  "name": "test-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "migrate": "npx prisma migrate dev --name test2 --schema ./src/prisma/schema.prisma",
    "generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "codegen": "graphql-codegen --config codegen.ts --debug",
