{
  "name": "test-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "/srv/bin/primary_tech/node dist/server/index.js",
    "startD": "react-scripts start",
    "build": "react-scripts build",
    "buildBE:prod": "tsc --project ./",
    "buildGen": "tsc codegenPlugins.ts --target es5",
    "buildFE:prod": "mv ./src/index.prod.tsx ./src/index.tsx && react-scripts build && rm -r public && mv build public",
    "migrate": "npx prisma migrate dev --name test2 --schema ./src/prisma/schema.prisma",
    "generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "codegen": "graphql-codegen --config codegen.ts --debug",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "serveDb": "npx prisma studio --schema ./src/prisma/schema.prisma",
    "pushDb": " npx prisma db push --schema ./src/prisma/schema.prisma",
    "srv": "nodemon --exec npx --esm ts-node ./src/server/index.ts",
    "startDev": "clear && concurrently \"npm run startD\" \"npm run srv\""
  },
  "dependencies": {
